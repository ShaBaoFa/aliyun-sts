{
    "name": "wlfpanda1012/aliyun-sts",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "Hyperf encapsulated using the latest version of the SDK for STS in the Alibaba Cloud openapi series interfaces with composer package.",
    "autoload": {
        "psr-4": {
            "Wlfpanda1012\\AliyunSts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "alibabacloud/sts-20150401": "^1.1.4",
        "jetbrains/phpstorm-attributes": "^1.0",
        "hyperf/framework": "^3.1",
        "hyperf/constants": "^3.1",
        "hyperf/config": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/codec": "^3.1",
        "hyperf/context": "^3.1",
        "hyperf/di": "^3.1",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "dev-master",
        "swow/swow": "dev-develop",
        "symfony/var-dumper": "^5.1"
    },
    "suggest": {
        "swow/swow": "Required to create swow components."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Wlfpanda1012\\AliyunSts\\ConfigProvider"
        }
    }
}
